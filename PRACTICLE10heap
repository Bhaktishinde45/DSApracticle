#include <iostream>
using namespace std;

int main() {
    const int n = 7;
    int marks[n];
    int maxArr[n], minArr[n];

    cout << "Enter marks: ";
    cin>>marks[n];
    for (int i = 0; i < n; i++) {
        cin >> marks[i];
        maxArr[i] = marks[i];
        minArr[i] = marks[i];
    }

    for (int i = n / 2 - 1; i >= 0; i--) {
        int largest = i;
        int left = 2 * i + 1;
        int right = 2 * i + 2;

        if (left < n && maxArr[left] > maxArr[largest])
            largest = left;

        if (right < n && maxArr[right] > maxArr[largest])
            largest = right;

        if (largest != i)
            swap(maxArr[i], maxArr[largest]);
    }


    for (int i = n / 2 - 1; i >= 0; i--) {
        int smallest = i;
        int left = 2 * i + 1;
        int right = 2 * i + 2;

        if (left < n && minArr[left] < minArr[smallest])
            smallest = left;

        if (right < n && minArr[right] < minArr[smallest])
            smallest = right;

        if (smallest != i)
            swap(minArr[i], minArr[smallest]);
    }

    // Output max and min values
    cout << "Maximum Marks: " << maxArr[0] << endl;
    cout << "Minimum Marks: " << minArr[0] << endl;

    return 0;
}
